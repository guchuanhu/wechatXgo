.head{
    background-color: white;
    padding: 16.38rpx 38.61rpx 10.38rpx 74.88rpx;
    margin-bottom: 11.7rpx;
}
.text{
    font-weight: bold;
    line-height: 46.8rpx;
    min-height: 46.8rpx;
    font-size: 28.08rpx;
}
.li {
    line-height: 42.12rpx;
    font-size: 22.23rpx;
}
.li::after{
    display: block;
    content: "";
    clear: both;
}
.left{
    float: left;
    max-width: 80%;
}
.left::before{
    position: relative;
    display: inline-block;
    content: "";
    margin-right: 20rpx;
    padding: 6rpx;
    border-radius: 15rpx;
    background-color: #09bb07;
}
.right{
    float: right;
}
.covering_position{
    height: 23.4rpx;
}
.li_fix{
    margin-top: 15rpx;
    color: rgb(187, 187, 187);
}
.down::after{
    content: ">";
    display: inline-block;
    position: relative;
    left: 8rpx;
    transform:scale(1.2,1) rotate(90deg) ;
}
.up::after{
    content: ">";
    display: inline-block;
    position: relative;
    left: 8rpx;
    transform:scale(1.2,1) rotate(-90deg) ;
}
.main{
    padding-bottom: 1rpx;
}
.main_dis{
    display: inline-block
}
.main_li{
    white-space: nowrap;
    display: inline-block;
    box-sizing: border-box;
    width: 33.33%;
    text-align: center;
    color: #09bb07;
    background-color: white;
    padding: 42.12rpx 0;
    border-top: 1rpx solid #09bb07;
    border-bottom: 1rpx solid #09bb07;
    border-right: 1rpx solid #09bb07;
}
.main_li_right{
    border-right: 0;
}
.item{
    background-color: #09bb07;
    color: white;
}
.font24{
    font-size: 28.08rpx;
}
.font18{
    font-size: 21.06rpx;
}
.marb8{margin-bottom: 8rpx;}
.border{
    border-right: 1rpx solid #09bb07;
    border-left: 1rpx solid #09bb07;
}
.btn_code{
    position: absolute;
    right: 30rpx;
    top: 23rpx;
    z-index: 9;
    height: 44.46rpx;
    padding-right: 16rpx;
    padding-left: 16rpx;
    font-size: 22.23rpx;
    line-height: 46.46rpx;
    border: 0;
    border-radius: 8rpx;
    background-color: #09bb07;
    color: white;
}
.btn_codeing{
    background-color: rgb(187, 187, 187);
}
.line{
    width: 100%;
    height: 11.7rpx;
    margin: 0 -23.4rpx;
    padding: 0 23.4rpx;
    background-color: rgb(245, 245, 249);
}
.location{
    float: right;
    color: rgb(136, 136, 136);
    margin-right: 30rpx;
}
.location::after{
    content: ">";
    display: inline-block;
    position: relative;
    top: -3rpx;
    margin-left: 6rpx;
    transform:scale(1,1.5);
    color: rgb(136, 136, 136);
}
.loca_change{
    float: right;
    color: rgb(136, 136, 136);
    position: relative;
    font-size: 25.74rpx;
    top:-35rpx;
}

.loca_change::after{
    content: ">";
    display: inline-block;
    position: relative;
    top: -3rpx;
    margin-left: 6rpx;
    transform:scale(1,1.5);
}
.disno{
    display: none;
}

.btn{
    margin-bottom: 100rpx;
}
.detail_input{
